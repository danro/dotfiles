snippet t
	<${1:tag}>${2}</$1>${3}
snippet tag
	<${1:tag}>
		${2}
	</$1>
snippet attr
	${1:attribute}="${2:property}"
snippet attr+
	${1:attribute}="${2:property}" attr+${3}
snippet a
	<a href="${1:#}">${2:$1}</a>${3}
snippet .
	class="${1}"${2}
snippet #
	id="${1}"${2}
snippet alt
	alt="${1}"${2}
snippet d
	<div>${1}</div>${2}
snippet div
	<div>
		${1}
	</div>
snippet sp
	<span>${1}</span>${2}
